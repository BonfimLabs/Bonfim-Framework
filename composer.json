{
    "name": "edsononildo/framework",
    "type": "library",
    "description": "",
    "keywords": [
        "EdsonOnildoJR",
        "PHP-Framework-Core"
    ],
    "homepage": "https://github.com/EdsonOnildoJR/PHP-Framework-Core",
    "license": "MIT",
    "authors": [
        {
            "name": "Edson Onildo",
            "email": "inbox.edsononildo@gmail.com",
            "homepage": "https://github.com/EdsonOnildoJR",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.1",
        "edsononildo/tpl": "3.0.x-dev",
        "edsononildo/router": "2.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "EdsonOnildo\\Framework\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EdsonOnildo\\Framework\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
